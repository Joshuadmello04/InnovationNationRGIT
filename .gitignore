# Create or edit .gitignore
echo "venv/" >> .gitignore
echo "*.pyd" >> .gitignore
echo "*.dll" >> .gitignore
echo "*.exe" >> .gitignore
echo "*.lib" >> .gitignore
echo "__pycache__/" >> .gitignore
echo "*.pyc" >> .gitignore
__pycache__/
outputs/*

# Add and commit the .gitignore file
git add .gitignore
git commit -m "Add .gitignore to exclude virtual environment and large binaries"